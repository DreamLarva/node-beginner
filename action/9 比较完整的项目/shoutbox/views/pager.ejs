<div id="pager">
    <% if(page.count > 1 ){ %>
        <% if(page.number){ %>
            <a id="prev" href="/?page=<%= page.number %>">Prev</a>
        <% } %>
        <% if( page.number < page.count -1 ){ %>
            <% if(page.number){ %>
                &nbsp;&nbsp;
            <% } %>
            <a href="/?page=<%= page.number+2 %>" id="next">Next</a>
        <% } %>
    <% }%>
</div>